{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": [".astro/types.d.ts", "**/*", "src/**/*.astro", "src/**/*.ts"],
  "exclude": ["dist", "node_modules"]
}
